{
	"id": "#bullet-fired-event.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Bot event occurring, when a bullet is fired",
	
	"javaType": "net.robocode2.json_schema.events.BulletFiredEvent",
	"extends": {
		"$ref": "event.json",
		"default": "bullet-fired-event"
	},
	"properties": {
		"bullet": {
			"description": "Bullet that was fired",
			"$ref": "../states/bullet-state.json"
		}
	},
	"required": [
		"bullet"
	]
}	