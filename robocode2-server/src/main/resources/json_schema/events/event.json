{
	"id": "#event.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Event occurring in the battle arena",

	"javaType": "net.robocode2.json_schema.events.Event",
	"properties": {
		"message-type": {
			"enum": [
				"scanned-bot-event",
				"bullet-hit-bot-event",
				"bullet-hit-bullet-event",
				"bullet-missed-event",
				"hit-by-bullet-event",
				"hit-bot-event",
				"death-event",
				"bot-death-event",
				"skipped-turn-event",
				"won-round-event"
			]
		}
	},
	"required": [
		"message-type"
	]
}