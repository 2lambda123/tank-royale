{
	"id": "#bullet-missed-event.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Bot event occuring, when one of its bullets missed, i.e. leaves the arena",
	
	"type": "object",
	"extends": {
		"$ref": "event.json",
		"default": "bullet-missed-event"
	},
	"properties": {
		"bullet": {
			"description": "Bullet that missed",
			"$ref": "../bullet-state.json"
		}
	},
	"required": [
		"bullet"
	]
}	