{
	"id": "#bot-death-event.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Bot event occurring, when any bot is destroyed",
	
	"javaType": "net.robocode2.json_schema.events.BotDeathEvent",
	"extends": {
		"$ref": "event.json",
		"default": "bot-death-event"
	},
	"properties": {
		"bot-id": {
			"description": "Unique id of the bot that got hit",
			"type": "integer"
		}
	},
	"required": [
		"bot-id"
	]
}	