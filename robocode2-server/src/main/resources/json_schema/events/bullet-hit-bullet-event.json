{
	"id": "#bullet-hit-bullet-event.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Bot event occurring, when the bullet from a bot hits another bullet",
	
	"javaType": "net.robocode2.json_schema.events.BulletHitBulletEvent",
	"extends": {
		"$ref": "event.json",
		"default": "bullet-hit-bullet-event"
	},
	"properties": {
		"bullet": {
			"description": "Bullet that hit another bullet",
			"$ref": "../states/bullet-state.json"
		},
		"hitBullet": {
			"description": "The other bullet that was hit by the bullet",
			"$ref": "../states/bullet-state.json"
		}
	},
	"required": [
		"bullet",
		"hitBullet"
	]
}	