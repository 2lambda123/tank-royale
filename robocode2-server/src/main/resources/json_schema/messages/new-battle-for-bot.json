{
	"id": "#new-battle-for-bot.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "New battle info for a bot. New game has started.",

	"javaType": "net.robocode2.json_schema.messages.NewBattleForBot",
	"extends": {
		"$ref": "message.json",
		"default": "new-battle-for-bot"
	},
	"properties": {
		"game-setup": {
			"description": "Game setup",
			"$ref": "../game-setup.json"
		},
		"my-id": {
			"description": "My ID, is an unique identifier for this bot",
			"type": "integer"
		}
	},
	"required": [
		"game-setup",
		"my-id"
	]
}
