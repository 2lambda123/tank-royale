{
	"id": "#message.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Message exchanged between server and client",

	"javaType": "net.robocode2.json_schema.messages.Message",
	"properties": {
		"type": {
			"enum": [
				"bot-handshake",
				"bot-info",
				"bot-intent",
				"bot-list",
				"bot-ready",
				"controller-handshake",
				"new-battle-for-bot",
				"new-battle-for-observer",
				"observer-handshake",
				"results-for-bot",
				"results-for-observer",
				"server-handshake",
				"tick-for-bot",
				"tick-for-observer"
			]
		}
	},
	"required": [
		"type"
	]
}