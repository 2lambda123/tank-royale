{
	"id": "#results-for-bot.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "All game results visible for a bot. Game has ended.",

	"javaType": "net.robocode2.json_schema.messages.ResultsForBot",
	"extends": {
		"$ref": "message.json",
		"default": "results-for-bot"
	},
	"properties": {
		"results": {
			"description": "Results of the battle",
			"type": "array",
			"items": { "$ref": "../bot-results-for-bot.json" }
		}
	},
	"required": [
		"results"
	]
}