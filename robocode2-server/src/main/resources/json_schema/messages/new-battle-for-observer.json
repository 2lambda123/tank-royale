{
	"id": "#new-battle-for-observer.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "New battle info for an observer. New game has started.",

	"javaType": "net.robocode2.json_schema.messages.NewBattleForObserver",
	"extends": {
		"$ref": "message.json",
		"default": "new-battle-for-observer"
	},
	"properties": {
		"game-setup": {
			"description": "Game setup",
			"$ref": "../game-setup.json"
		},
		"participants": {
			"description": "List of bots participating in battle",
			"type": "array",
			"items": { "$ref": "../participant.json" }
		}
	},
	"required": [
		"game-setup",
		"participants"
	]
}