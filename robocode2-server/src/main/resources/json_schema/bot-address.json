{
	"id": "#bot-address.json",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Bot address",

	"javaType": "net.robocode2.json_schema.BotAddress",
	"properties": {
		"host-name": {
			"description": "Host name",
			"type": "string"
		},
		"port": {
			"description": "Port number",
			"type": "integer"
		}
	},
	"required": [
		"host-name",
		"port"
	]
}
